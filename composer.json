{
    "name": "realpage/json-api-for-lumen",
    "type": "library",
    "description": "Lumen helpers to help ease implementation of json-api standards",
    "keywords": [
        "lumen",
        "json-api",
        "realpage"
    ],
    "homepage": "https://github.com/realpage/json-api-for-lumen",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "neomerx/json-api": "3.*",
        "illuminate/http": "^5.4",
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.8",
        "mockery/mockery": "1.0.0-alpha1"
    },
    "autoload": {
        "psr-4": {
            "RealPage\\JsonApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RealPage\\JsonApi\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
